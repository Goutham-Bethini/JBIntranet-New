@model IEnumerable<USPS_Report.Areas.Reports.Models.InactiveAccuntVM>

@{
    ViewBag.Title = "RONotPurchaseReport";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="middle">
    <h1>Reocurring work orders not setup as purchase</h1>
</div>

<div class="row">
    <div class="col-md-2"></div>
    <div class="col-md-10">
        <div class=" row">
            <h3 style="color:blue">

                @Model.Count() Reocurring work orders not setup as purchase.
            </h3>
        </div>
        <hr />
        <div class=" row">

            <div class="col-md-5 table-responsive">
                <table data-sortable class="table table-hover table-striped" style="border:groove;width:300px">
                    <tr>
                        <th>
                            Account
                        </th>

                    </tr>

                    @foreach (var item in Model)
            {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.Account)
                            </td>

                        </tr>
                    }

                </table>
            </div>

        </div>
    </div>
</div>

<style>
    .middle {
        margin: auto;
        width: 900px;
    }

      th {
        background-color: dodgerblue;
        color: white;
    }
</style>



