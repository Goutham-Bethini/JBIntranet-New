@model USPS_Report.Areas.Reports.Models.EligSchVM

@{
    ViewBag.Title = "EligSchedule";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>EligSchedule</h2>
<div class="table-responsive">
    <table data-sortable class="table table-hover table-striped table-bordered" style="width:90%; align-content:center">
        <thead>
            <tr>
              
                <th>Date</th>
                <th>Schedule Orders</th>
                <th>Diab</th>
                <th>NonDiab</th>
                <th>Diab_NonDiab</th>
              

            </tr>
        </thead>
        <tbody>
            @foreach (var rec in Model._schOrders)
            {
                <tr>
                    <td>@rec.nextDate</td>
                    <td>@rec.count</td>
                    <td>@rec.Diab</td>
                    <td>@rec.NonDiab</td>
                    <td>@rec.Diab_NonDiab</td>
                    
                </tr>
            }
        </tbody>

    </table>
</div>



