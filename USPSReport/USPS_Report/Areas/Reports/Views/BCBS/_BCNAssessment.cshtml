                   <br/>
                    @*<input type="button" class="btn btn-primary" value="SAVE" id="diabsave" style="text-align:center" /> &nbsp;&nbsp;*@
                    <input type="button" class="btn btn-primary" id="diabbtn_close" value="CLOSE" style="width:100%" />
    <br/>
    </center>        
        </div> @*modal body*@   
