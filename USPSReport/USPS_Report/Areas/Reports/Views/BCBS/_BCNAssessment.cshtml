
@model USPS_Report.Areas.Reports.Controllers.BCNCallLogVM
@using Kendo.Mvc.UI
@*<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>*@
<br/>
<div class="row">
    <div class="col-md-1"></div>
  <div class="col-md-2">
      <strong>Assessment Completed with:</strong>
  </div>
  <div class="col-md-2">
      @Html.TextBoxFor(m => m.Assmnt_completedtxt)
  </div>
</div>
<br/>
<div class="row">
    <div class="col-md-1"></div>
    <div class="col-md-2">
        <strong>Relationship to member:</strong>
    </div>
    <div class="col-md-2">
    @Html.TextBoxFor(m => m.Assmnt_reltomember)
    </div>
</div>
<br />
<div class="row">
    <div class="col-md-1"></div>
    <div class="col-md-2">
        <strong>Education Done By:</strong>
    </div>
    <div class="col-md-2">
        @Html.TextBoxFor(m => m.Assmnt_edudoneby)
    </div>
</div>
<br />
<div class="row">
    <div class="col-md-1"></div>
    <div class="col-md-2">
        <strong>Does the member feel adequately trained on products :</strong>
    </div> 
    <div class="col-md-1">
        @Html.RadioButtonFor(m => m.Assmnt_memtrained, true) <label>Yes</label> &nbsp;&nbsp;&nbsp;&nbsp;
        @Html.RadioButtonFor(m => m.Assmnt_memtrained, false) <label>No</label>     
    </div>  
    </div>
<br />

<div class="row">
    <div class="col-md-1"></div>
    <div class="col-md-2">
        <strong>Physician for supplies:</strong>
    </div>
    <div class="col-md-2">
        @Html.TextBoxFor(m => m.Assmnt_physicianforsupp)
        @Html.HiddenFor(m => m.IsDiab)
        @Html.HiddenFor(m => m.IsInsulin)
    </div>
</div>
<br/>
<div class="row">
    <div class="col-md-1"></div>
    <div class="col-md-2">
        <strong>Who are they currently getting supplies from?</strong>
    </div>
    <div class="col-md-2">
        @Html.TextBoxFor(m => m.Assmnt_currsuppfrom)
    </div>
</div>
<br/>
<div class="row">
    <div class="col-md-1"></div>
    <div class="col-md-2">
        <input type="button" class="btn btn-primary" value="Diabetic" id="diabbutton"/>      
    </div>
</div>
<br/>
<div class="row">
    <div class="col-md-1"></div>
    <div class="col-md-2">
        <input type="button" class="btn btn-primary" value="Insulin Pump/CGM" id="inspumpbutton"/>
    </div>
</div>
<br />
<div class="row">
    <div class="col-md-1"></div>
    <div class="col-md-6">
        <strong>
            "TO ENSURE YOUR INSURANCE MAKES PAYMENT AND THAT YOU ARE NOT BILLED
            FOR SUPPLIES YOU MUST ADVISE J&B IF YOU RECEIVE ANY OF THESE SUPPLIES FROM ANOTHER SUPPLIER BEFORE EACH ORDER SHIPS"
        </strong>
    </div>
</div>
<br/>
<div class="row">
    <div class="col-md-1"></div>
    <div class="col-md-2">
        <strong>Date last order received:</strong>
    </div>
    <div class="col-md-2">
        @*@Html.TextBoxFor(m => m.Assmnt_lastorderrecieveddt)*@

        @*<input type="date" value = @Model.Assmnt_lastorderrecieveddt id="lastorderdt"/>*@    
        @*@Html.TextBoxFor( m => m.Assmnt_lastorderrecieveddt, new {@type="date", @Value = Model.Assmnt_lastorderrecieveddt})*@   
   
        @Html.TextBoxFor(m => m.Assmnt_lastorderrecieveddt)
                          
    </div>
</div>
<br/>
<div class="row">
    <div class="col-md-1"></div>
    <div class="col-md-2">
        <strong>What supplier was the order from:</strong>
    </div>
    <div class="col-md-2">
    @Html.TextBoxFor(m => m.Assmnt_supporderfrom)
    </div>
</div>
<br/>
<div class="row">
<div class="col-md-1"></div>
<div class="col-md-2">
    <strong>Was it a 30 or 90 day order :</strong>
</div>
    <div class="col-md-2">
        @*@Html.CheckBoxFor(m => m.Assmnt_memtrained) <label>Yes</label>*@
        @Html.RadioButtonFor(m => m.Assmnt_Ord30or90, 30) <label>30</label> &nbsp;&nbsp;&nbsp;&nbsp;
        @Html.RadioButtonFor(m => m.Assmnt_Ord30or90, 90) <label>90</label>
    </div>
</div>
<br />
<div class="row">
    <div class="col-md-1"></div>
    <div class="col-md-2">
        <strong>List all Remaining Supplies:</strong>
    </div>
    <div class="col-md-2">
        @Html.TextAreaFor(m => m.Assmnt_remsupplies, new {rows= "6", cols ="40"})
      
    </div>
    <div style="display:none">  @Html.CheckBoxFor(m => m.assmnt_Diab)</div>
    <div style="display:none">  @Html.CheckBoxFor(m => m.assmnt_Insulin)</div>
</div>
<br />

