<script>

    $('#Relation').click(function () {
        var $this = $(this);

        if ($this.val()=='Other') {
            $('#otherRelname').removeAttr("disabled");
        } else {
            $('#otherRelname').attr("disabled", "disabled")
        }
        });



</script>

